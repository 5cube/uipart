{
  "repository": "https://github.com/5cube/upart",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn install && lerna bootstrap",
    "release": "lerna version --conventional-commits --yes",
    "clean": "lerna clean --yes",
    "lerna": "lerna",
    "playground": "lerna run playground --scope=uipart --stream",
    "playground:build": "lerna run playground:build --scope=uipart --stream",
    "build": "lerna run build --scope=uipart --stream"
  },
  "engines": {
    "node": "^12 || ^14",
    "yarn": "^1.12"
  },
  "devDependencies": {
    "lerna": "^3.22.1",
    "typescript": "^4.1.3"
  }
}
